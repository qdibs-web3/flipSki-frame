{
    "name": "flipski",
    "version": "1.0.0",
    "description": "FlipSki - Blockchain-based coin flip game",
    "private": true,
    "scripts": {
      "start": "cd frontend && npm start",
      "build": "cd frontend && npm run build",
      "install-all": "npm install && cd frontend && npm install",
      "dev": "concurrently \"cd frontend && npm run dev\" \"vercel dev\"",
      "dev:frontend": "cd frontend && npm run dev",
      "dev:api": "vercel dev"
    },
    "engines": {
      "node": ">=18.0.0"
    },
    "dependencies": {
      "concurrently": "^8.2.2"
    },
    "author": "",
    "license": "ISC"
  }